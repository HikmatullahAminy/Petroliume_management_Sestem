@model IEnumerable<AHMSApplicationDemo.Models.Store>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a href="#">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OilName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalLiter)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OilName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalLiter)
            </td>
         
            <td>
                <a href="#" >Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StoreId">Details</a> |
                <a href="#" class="disabled">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
